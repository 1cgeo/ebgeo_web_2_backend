{
    "name": "ebgeo-backend",
    "version": "1.0.0",
    "description": "Backend para o sistema EBGEO",
    "main": "dist/index.js",
    "type": "module",
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "scripts": {
        "start": "node dist/index.js",
        "dev": "nodemon src/index.ts",
